#include <iostream>
#include <time.h>
#include <stdio.h>
#include <stdlib.h>
using namespace std;

void print_array(int *array, const int size){
    for (int i = 0; i < size; i++) {
        cout << array[i] << " ";
    }
}


int *F1(int a1, int b, const int size){
    int *arr = new int[size];
    for (int i = 0; i < size; i++){
        arr[i] = a1;
        a1 += b;
    }
    return arr;
}

int *F2(const int N, int g = 0){
    int* a = new int[N];
    int j;
    srand(time(NULL));
    for(int i=0; i<N; i++) a[i] = i+1+g;
    for(int k=N; k>1; k--) {
        j = rand()%k;
        int t = a[k-1];
        a[k-1] = a[j];
        a[j] = t;
    }
    return a;

}

int F3(){
    int* a = F2(1000,1000);
    int b = 0;
    for (int i = 0; i < 1000; i++){
        if (((a[i]/10)%10)%2 == 0){
            b++;
        }
    }
    return b;
}

int main() {
    system("chcp 65001");
    int x, y, size;
    cin >> x >> y >> size;
    int* a = F1(x,y, size);
    print_array(a,size);
    delete [] a;
    cout << endl;

    int N;
    cin >> N;
    int* b = F2(N);
    print_array(b, N);
    cout << endl;
    delete [] b;
    cout << F3();
    return 0;
}
